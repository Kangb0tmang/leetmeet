<h1>Events#index</h1>
<p>Find me in app/views/events/index.html.erb</p>

<div class="events_index_filters">
  <select name="" id="">
    <% @games.each do |game| %>
      <option value=""> <%= game.name %> </option>
    <% end %>
  </select>
</div>
<div class="events_index_event_list">
  <% @events.each do |event| %>
    <div class="events_index_event">
      <p>
        <%= event.title %>
        <%= event.user.username %>
        <%= event.location %>
        <%= event.game.name %>
        <%= event.category.name %>
      </p>
    </div>
  <% end %>
    
</div>
